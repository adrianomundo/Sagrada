<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?scenebuilder-stylesheet application.css?>

<AnchorPane fx:id="controller" prefHeight="400.0" prefWidth="600.0" stylesheets="application.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.se2018.client.view.login.LoginController">
   <children>
      <GridPane layoutX="166.0" layoutY="58.0" prefHeight="142.0" prefWidth="308.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="150.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="159.0" minWidth="0.0" prefWidth="150.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <RadioButton fx:id="ViewCli" mnemonicParsing="false" text="CLI" />
            <RadioButton fx:id="ViewGui" mnemonicParsing="false" text="GUI" GridPane.columnIndex="1" />
            <RadioButton fx:id="Socket" mnemonicParsing="false" text="Socket" GridPane.rowIndex="1" />
            <RadioButton fx:id="Rmi" mnemonicParsing="false" text="RMI" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="268.0" layoutY="281.0" mnemonicParsing="false" onAction="#startClient" text="Login" />
